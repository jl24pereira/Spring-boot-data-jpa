<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE cempi [

<!ELEMENT cempi (property+|db|mq)*>
<!ATTLIST cempi
 banco CDATA #REQUIRED
 descripcion CDATA #IMPLIED
 >

<!ELEMENT property EMPTY>
<!ATTLIST property
 name CDATA #REQUIRED
 value CDATA #REQUIRED
 >

<!ELEMENT db EMPTY>
<!ATTLIST db
 db-name CDATA #REQUIRED
 password CDATA #REQUIRED
 user CDATA #REQUIRED
 url CDATA #REQUIRED
 driver CDATA #REQUIRED
 max-connections CDATA #IMPLIED
 checkout-timeout CDATA #IMPLIED
 max-checkout CDATA #IMPLIED
 >

<!ELEMENT mq EMPTY>
<!ATTLIST mq
 transaccion CDATA #REQUIRED
 timeout CDATA #REQUIRED
 queue-request CDATA #REQUIRED
 queue-requestnight CDATA #REQUIRED
 queue-response CDATA #REQUIRED
 queue-reversion CDATA #REQUIRED
 queue-reversionnight CDATA #REQUIRED
 qm-request CDATA #REQUIRED
 qm-response CDATA #REQUIRED
 qm-reversion CDATA #REQUIRED
 hacerreversion CDATA #REQUIRED
 >

]>
